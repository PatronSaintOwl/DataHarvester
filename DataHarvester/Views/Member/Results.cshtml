@model DataHarvester.Model.ResultMember
@{
    ViewBag.Title = "Results";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@*Title : Result*@
<section class="welcome p-t-10">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1 class="title-4">Sonuçlar</h1>
                <hr class="line-seprate">
            </div>
        </div>
    </div>
</section>

@*Email - AllEmail - Host - File*@
<section class="statistic statistic2">
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-lg-3">
                <div class="statistic__item statistic__item--green">
                    <h2 class="number">@Model.ResultEmailList.Count()</h2>
                    <span class="desc">Bulunan EMAİL Sayısı (Eşleşen)</span>
                    <div class="icon">
                        <i class="zmdi zmdi-email"></i>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-3">
                <div class="statistic__item statistic__item--blue">
                    <h2 class="number">@Model.ResultAllEmailList.Count()</h2>
                    <span class="desc">Bulunan EMAİL Sayısı</span>
                    <div class="icon">
                        <i class="zmdi zmdi-email-open"></i>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-3">
                <div class="statistic__item statistic__item--red">
                    <h2 class="number">@Model.ResultHostnameList.Count()</h2>
                    <span class="desc">Bulunan Host Sayısı</span>
                    <div class="icon">
                        <i class="zmdi zmdi-dns"></i>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-3">
                <div class="statistic__item statistic__item--orange">
                    <h2 class="number">@Model.ResultFileUrlList.Count()</h2>
                    <span class="desc">Bulunan Dosya Sayısı</span>
                    <div class="icon">
                        <i class="zmdi zmdi-file"></i>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
@*IP - Port - LinkedIn Profile / Link*@
<section class="statistic statistic2">
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-lg-3">
                <div class="statistic__item statistic__item--purple">
                    <h2 class="number">@Model.ResultIPList.Count()</h2>
                    <span class="desc">Bulunan IP Sayısı</span>
                    <div class="icon">
                        <i class="zmdi zmdi-n-4-square"></i>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-3">
                <div class="statistic__item statistic__item--yellow">
                    <h2 class="number">@Model.ResultPortList.Count()</h2>
                    <span class="desc">Bulunan Port Sayısı</span>
                    <div class="icon">
                        <i class="zmdi zmdi-n-1-square"></i>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-3">
                <div class="statistic__item statistic__item--pink">
                    <h2 class="number">@Model.ResultLinkedInProfileList.Count()</h2>
                    <span class="desc">Bulunan LinkedIn Profile Sayısı</span>
                    <div class="icon">
                        <i class="zmdi zmdi-linkedin-box"></i>
                    </div>
                </div>
            </div>

            <div class="col-md-6 col-lg-3">
                <div class="statistic__item statistic__item--darkblue">
                    <h2 class="number">@Model.ResultLinkedInLinkList.Count()</h2>
                    <span class="desc">Bulunan LinkedIn Link Sayısı</span>
                    <div class="icon">
                        <i class="zmdi zmdi-linkedin"></i>
                    </div>
                </div>
            </div>

        </div>
    </div>
</section>

@*Table : Email - AllEmail*@
<section class="statistic statistic2">
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <div class="au-card au-card--no-shadow au-card--no-pad m-b-40">
                    <div class="au-card-title">
                        <div class="bg-overlay bg-overlay--green"></div>
                        <h3>
                            <i class="zmdi zmdi-email"></i>Email Listesi (Eşleşen)
                        </h3>
                    </div>
                    <div class="au-task js-list-load">
                        <div class="au-task__title">
                            <p>"@Model.SearchQuery" domainiyle eşleşen mailler (@Model.ResultEmailList.Count() sonuç) </p>
                        </div>
                        <div class="au-task-list js-scrollbar3">

                            @foreach (var mail in Model.ResultEmailList)
                            {
                                <div class="au-task__item au-task__item--green">
                                    <div class="au-task__item-inner">
                                        <h5 class="task">
                                            <a href="mailto:@mail" target="_blank">@mail</a>
                                        </h5>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="au-card au-card--no-shadow au-card--no-pad m-b-40">
                    <div class="au-card-title">
                        <div class="bg-overlay bg-overlay--blue"></div>
                        <h3>
                            <i class="zmdi zmdi-email-open"></i>Tüm Email Listesi
                        </h3>
                    </div>
                    <div class="au-task js-list-load">
                        <div class="au-task__title">
                            <p>"@Model.SearchQuery" domainiyle ilişkili tüm mail formatları (@Model.ResultAllEmailList.Count() sonuç)</p>
                        </div>
                        <div class="au-task-list js-scrollbar3">

                            @foreach (var mail in Model.ResultAllEmailList)
                            {
                                <div class="au-task__item au-task__item--blue">
                                    <div class="au-task__item-inner">
                                        <h5 class="task">
                                            <a href="mailto:@mail" target="_blank">@mail</a>
                                        </h5>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

